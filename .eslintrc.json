{
  "parser": "babel-eslint",
  "rules": {
    "no-new": 0,
    "no-extra-boolean-cast": 0,
    "semi": 0, 
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "ignore",
        "asyncArrow": "always"
      }
    ],
    "require-jsdoc":"off",
    "camelcase":"warn",
    "eol-last":["error", "never"]
  }
}